<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xbstar.esl.dao.ConferenceMapper">
	<resultMap id="BaseResultMap3"
		type="com.xbstar.esl.domain.Conference">
		<result column="conf_name" jdbcType="VARCHAR"
			property="confName" />
		<result column="member_id" jdbcType="VARCHAR"
			property="memberId" />
		<result column="createtime" jdbcType="VARCHAR"
			property="createTime" />
		<result/>
		<result column="is_video" jdbcType="VARCHAR"
			property="isVideo" />
		<result/>
			
	</resultMap>
	
	<sql id="Base_Column_List3">
			conf_name,member_id,createtime,is_video
	</sql>

	<insert id="insertConf" parameterType="com.xbstar.esl.domain.Conference">
		insert into conference(conf_name,member_id,createtime,is_video)
		values(#{confName,jdbcType=VARCHAR},#{memberId,jdbcType=VARCHAR},#{createTime,jdbcType=VARCHAR},#{isVideo,jdbcType=VARCHAR})
	
	</insert>
</mapper>